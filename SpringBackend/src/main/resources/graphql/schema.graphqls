type Query {
    events: [Event]
    eventById(eventID: Int): Event
    courses: [Course]
    courseByName(courseName: String): Course
}

type Event {
    eventID: Int
    dateInfo: String
    timeInfo: String
    openTo: String
    eventDesc: String
    relevantLinks: String
}

type Course {
    courseName: String
    courseDesc: String
    price: Float
    duration: String
    capacity: Int
    targetAge: String
}